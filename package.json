{
    "name": "typescript-action",
    "version": "0.1.1",
    "private": true,
    "description": "TypeScript template action",
    "bin": {
        "template": "./bin/get-oneflow"
    },
    "files": [
        "/bin",
        "/lib",
        "/npm-shrinkwrap.json",
        "/oclif.manifest.json",
        "/action.yml",
        "/tsconfig.json",
        "/oclif.sh"
    ],
    "main": "lib/main.js",
    "scripts": {
        "build": "tsc",
        "format": "prettier --write **/*.ts",
        "format-check": "prettier --check **/*.ts",
        "lint": "eslint src/**/*.ts",
        "package-main": "ncc build --minify -o dist src/main.ts",
        "package": "concurrently 'npm:package-*'",
        "test": "jest",
        "all": "npm run build && npm run format && npm run lint && npm run package && npm test",
        "fast": "npm run build && npm run package && npm test",
        "test-ssh": "npm run build && TEST_SSH=1 npm run test -- __tests__/ssh.test.ts",
        "test-cuda": "TEST_MANYLINUX='1' npm run test -- __tests__/cuda.test.ts",
        "test-hash": "npm run build && npm run test -- __tests__/hash.test.ts",
        "pub": "rm -rf dist && rm -rf lib && npm run all && git add . && git commit -m publishing && git push",
        "command-build": "shx rm -rf lib && tsc -b",
        "command-run": "bash +x oclif.sh",
        "command-pack": "npm run command-build && oclif manifest && oclif readme && oclif pack tarballs && shx rm -f oclif.manifest.json",
        "command-test": "tar -zxvf dist/*linux-x64.tar.gz && ./get-oneflow/bin/get-oneflow build --help && rm -rf get-oneflow/"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/actions/typescript-action.git"
    },
    "keywords": [
        "actions",
        "node",
        "setup"
    ],
    "author": "",
    "license": "MIT",
    "dependencies": {
        "@actions/core": "^1.5.0",
        "@actions/exec": "^1.1.0",
        "@actions/github": "^5.0.0",
        "@actions/glob": "^0.2.0",
        "@actions/io": "^1.1.1",
        "@actions/tool-cache": "^1.7.1",
        "ali-oss": "^6.16.0",
        "cli-table3": "^0.6.0",
        "dockerode": "^3.3.0",
        "node-ssh": "^12.0.0",
        "ssh2-sftp-client": "^7.0.4",
        "uuid": "^8.3.2",
        "@oclif/core": "^1",
        "@oclif/plugin-help": "^5",
        "@oclif/plugin-plugins": "^2.0.1",
        "js-yaml": "^4.1.0",
        "ts-node": "^10.2.1"
    },
    "devDependencies": {
        "@types/ali-oss": "^6.0.10",
        "@types/dockerode": "^3.2.7",
        "@types/js-yaml": "^4.0.3",
        "@types/node": "^16.3.3",
        "@types/semver": "^7.3.8",
        "@types/ssh2": "^0.5.48",
        "@types/ssh2-sftp-client": "^7.0.0",
        "@types/uuid": "^8.3.1",
        "@typescript-eslint/parser": "^4.28.3",
        "@vercel/ncc": "^0.30.0",
        "concurrently": "^6.2.1",
        "eslint": "^7.31.0",
        "eslint-plugin-github": "^4.1.1",
        "eslint-plugin-jest": "^24.3.6",
        "jest": "^27.0.6",
        "prettier": "2.2.1",
        "ts-jest": "^27.0.3",
        "typescript": "^4.1.3",
        "@oclif/test": "^2",
        "@types/chai": "^4",
        "@types/mocha": "^9.0.0",
        "chai": "^4",
        "eslint-config-oclif": "^4",
        "eslint-config-oclif-typescript": "^1.0.2",
        "globby": "^11",
        "mocha": "^9",
        "oclif": "^2",
        "shx": "^0.3.3",
        "tslib": "^2.3.1"
    },
    "oclif": {
        "bin": "get-oneflow",
        "dirname": "get-oneflow",
        "commands": "./lib/commands",
        "helpClass": "./lib/commands-help",
        "plugins": [
            "@oclif/plugin-help",
            "@oclif/plugin-plugins"
        ],
        "topicSeparator": " "
    }
}
